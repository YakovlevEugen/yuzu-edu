{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "dev": {
      "passThroughEnv": ["*"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**", "tsconfig.tsbuildinfo", "out/**"],
      "cache": true
    },
    "deploy:staging": {
      "dependsOn": ["build"],
      "passThroughEnv": ["*"],
      "cache": true
    }
  }
}
