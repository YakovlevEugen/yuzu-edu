{
  "name": "@yuzu/web",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "build-storybook": "storybook build",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf .turbo .wrangler build node_modules",
    "dev": "tunnel vite",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --check --ignore-path .gitignore \"**/*.+(html|json|mdx|yml)\"",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "stylelint": "stylelint -f verbose --ignore-path .gitignore \"**/*.+(css|scss)\"",
    "svgo": "svgo -f ./src/assets/svg --config=./svgo.config.cjs",
    "test": "vitest",
    "test-storybook": "test-storybook",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production"
  },
  "cloudflared": {
    "name": "yuzu-web-$USER",
    "hostname": "yuzu-web-$USER.r8edev.xyz",
    "target": "http://localhost:5173"
  },
